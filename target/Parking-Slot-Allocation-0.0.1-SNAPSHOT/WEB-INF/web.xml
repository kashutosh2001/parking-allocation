<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Parking-Slot-Allocation</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>User</servlet-name>
    <servlet-class>com.yash.parkingslot.controller.UserRegistration</servlet-class>
  </servlet>
  
  <servlet-mapping>
     <servlet-name>User</servlet-name>
     <url-pattern>/userRegistration</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.yash.parkingslot.controller.UserLogin</servlet-class>
  </servlet>
  
  <servlet-mapping>
     <servlet-name>Login</servlet-name>
     <url-pattern>/userLogin</url-pattern>
  </servlet-mapping>
  
</web-app>